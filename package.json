  "homepage": "https://github.com/pangju666/js-utils#readme",
  "dependencies": {
    "qs": "^6.10.1",
    "dateformat": "^4.5.1",
    "axios": "^0.21.1",
    "js-cookie": "^2.2.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "jest": "^27.0.4",
    "prettier": "^2.3.1",
    "ts-jest": "^27.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.4"
  },
  "files": ["lib/**/*"]
