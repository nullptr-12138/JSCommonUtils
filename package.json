{
  "name": "jc-utils",
  "version": "1.0.0",
  "description": "tools",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "lint": "eslint 'src/**/*.{js,ts}'",
    "dev": "rollup -w -c --environment FORMAT:esm",
    "build:esm": "rollup -c --environment FORMAT:esm",
    "build:umd": "rollup -c --environment FORMAT:umd",
    "build:min": "rollup -c --environment FORMAT:min",
    "build": "rimraf lib/* && run-p build:esm build:umd build:min"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*./src/**/*.{js,ts,json,css,less,md}": [
      "prettier --write",
      "npm lint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pangju666/js-utils.git"
  },
  "keywords": [
    "utils",
    "tools"
  ],
  "author": "pangju",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pangju666/js-utils/issues"
  },
  "homepage": "https://github.com/pangju666/js-utils#readme",
  "dependencies": {
    "qs": "^6.10.1",
    "dateformat": "^4.5.1",
    "axios": "^0.21.1",
    "js-cookie": "^2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "eslint": "^7.29.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "lodash.merge": "^4.6.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.1",
    "rollup-plugin-babel": "^4.4.0",
    "typescript": "^4.3.4"
  },
  "files": [
    "lib/**/*"
  ]
}
